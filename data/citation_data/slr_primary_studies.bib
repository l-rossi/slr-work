@inproceedings{PS_0180,
	address = {New York, NY, USA},
	series = {{ICAIL} '11},
	title = {Towards formalising argumentation about legal cases},
	isbn = {978-1-4503-0755-0},
	url = {https://doi.org/10.1145/2018358.2018359},
	doi = {10.1145/2018358.2018359},
	abstract = {In this paper we offer an account of reasoning with legal cases in terms of argumentation schemes. These schemes, and undercutting attacks associated with them, are expressed as defeasible rules of inference that will lend themselves to formalisation within the AS-PIC+ framework. We begin by modelling the style of reasoning with cases developed by Aleven and Ashley in the CATO project, which describes cases using factors, and then extend the account to accommodate the dimensions used in Rissland and Ashley's earlier HYPO project. Some additional scope for argumentation is then identified and formalised.},
