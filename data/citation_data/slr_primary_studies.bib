	pages = {376--381},
}

@inproceedings{PS_0150,
	address = {New York, NY, USA},
	series = {{ICAIL} '07},
	title = {Constructing arguments with a computational model of an argumentation scheme for legal rules: interpreting legal rules as reasoning policies},
	isbn = {978-1-59593-680-6},
