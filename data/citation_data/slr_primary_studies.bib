	pages = {103--110},
}

@inproceedings{PS_0331,
	address = {New York, NY, USA},
	series = {{ICAIL} '19},
	title = {{MagnetDroid}: security-oriented analysis for bridging privacy and law for {Android} applications},
	isbn = {978-1-4503-6754-7},
	url = {https://doi.org/10.1145/3322640.3326729},
