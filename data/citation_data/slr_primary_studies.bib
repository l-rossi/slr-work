@inproceedings{PS_0280,
	address = {New York, NY, USA},
	series = {{ICAIL} '93},
	title = {Identification of implicit legal requirements with legal abstract knowledge},
	isbn = {0-89791-606-9},
	url = {https://doi.org/10.1145/158976.159013},
